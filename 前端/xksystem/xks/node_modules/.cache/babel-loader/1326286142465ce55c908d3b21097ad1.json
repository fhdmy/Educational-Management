AAA,EAAA;AACA,aAAA,YAAA,GAAA,KAAA;AACA,aAAA,UAAA,GAAA,IAAA;AACA,aAAA,GAAA,GAAA,YAAA,CAAA,OAAA,CAAA,KAAA,CAAA;AACA;AACA,KAXA;AAYA,IAAA,MAZA,oBAYA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,KAdA;AAeA,IAAA,MAfA,oBAeA;AACA,UAAA,OAAA,CAAA,QAAA,CAAA,EAAA;AACA,aAAA,UAAA,GAAA,KAAA;AACA,aAAA,GAAA,GAAA,EAAA;AACA,QAAA,YAAA,CAAA,UAAA,CAAA,KAAA;AACA,QAAA,YAAA,CAAA,UAAA,CAAA,WAAA;AACA;AACA;AAtBA;AAjBA,CAAA","sourcesContent":["<template>\n  <v-app id=\"index\">\n    <v-navigation-drawer v-model=\"drawer\" app>\n      <v-list dense>\n        <v-list-item link @click=\"panel(0)\">\n          <v-list-item-action>\n            <v-icon>mdi-home</v-icon>\n          </v-list-item-action>\n          <v-list-item-content>\n            <v-list-item-title>首页</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n        <v-list-item link @click=\"panel(1)\">\n          <v-list-item-action>\n            <v-icon>mdi-contact-mail</v-icon>\n          </v-list-item-action>\n          <v-list-item-content>\n            <v-list-item-title>选课信息</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n    </v-navigation-drawer>\n\n    <v-app-bar app color=\"indigo\" dark>\n      <v-app-bar-nav-icon @click.stop=\"drawer = !drawer\"/>\n      <v-toolbar-title>选课系统</v-toolbar-title>\n      <v-spacer></v-spacer>\n      <v-toolbar-title class=\"login-btn\" @click=\"login_dialog=true\" v-show=\"!login_flag\">登录</v-toolbar-title>\n      <v-toolbar-title class=\"login-btn\" @click=\"logout()\" v-show=\"login_flag\">注销</v-toolbar-title>\n      <v-dialog v-model=\"login_dialog\" width=\"500\">\n        <Login @login=\"login\" @cancel=\"cancel\"></Login>\n      </v-dialog>\n    </v-app-bar>\n\n    <v-content style=\"padding:20px;\">\n      <ChooseCourse v-if=\"panel_flag==0\" :sid=\"sid\"></ChooseCourse>\n      <Calendar v-if=\"panel_flag==1\" :sid=\"sid\"></Calendar>\n    </v-content>\n    <v-footer color=\"indigo\" app>\n      <span class=\"white--text\">&copy; 2020</span>\n    </v-footer>\n  </v-app>\n</template>\n\n<script>\nimport ChooseCourse from \"./ChooseCourse\";\nimport Login from \"./Login\";\nimport Calendar from \"./Calendar\"\n\nexport default {\n  name: \"Index\",\n  props: {\n    source: String\n  },\n  components: {\n    ChooseCourse,\n    Login,\n    Calendar\n  },\n  data: () => ({\n    drawer: null,\n    login_dialog: false,\n    panel_flag:0,\n    login_flag:false,\n    sid:\"\"\n  }),\n  methods: {\n    panel(i){\n      this.panel_flag=i;\n    },\n    login(e){\n      //login success\n      if(e){\n        this.login_dialog=false;\n        this.login_flag=true;\n        this.sid=localStorage.getItem(\"sid\");\n      }\n    },\n    cancel(){\n      this.login_dialog=false;\n    },\n    logout(){\n      if(confirm('确定要注销?')) {\n        this.login_flag=false;\n        this.sid=\"\";\n        localStorage.removeItem(\"sid\");\n        localStorage.removeItem(\"spassword\");\n      }\n    }\n  },\n};\n</script>\n\n<style scoped>\n.login-btn {\n  margin-right: 20px;\n  cursor: pointer;\n}\n</style>\n"],"sourceRoot":"src/components"}]}