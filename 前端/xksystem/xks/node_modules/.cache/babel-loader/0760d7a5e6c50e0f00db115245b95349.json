 v-slot:item.action=\"{ item }\">\n        <v-btn @click=\"delete_item(item)\" color=\"primary\">退课</v-btn>\n      </template>\n    </v-data-table>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: [\"sid\"],\n  data: () => ({\n    headers: [\n      {\n        text: \"人数\",\n        align: \"start\",\n        value: \"pnum\"\n      },\n      {\n        text: \"课程\",\n        value: \"course\"\n      },\n      {\n        text: \"教师\",\n        value: \"teacher\"\n      },\n      {\n        text: \"上课时间\",\n        value: \"time\"\n      },\n      {\n        text: \"地点\",\n        value: \"place\"\n      },\n      {\n        text: \"学分\",\n        value: \"score\"\n      },\n      {\n        text: \"行为\",\n        value: \"action\",\n        sortable: false\n      }\n    ],\n    courses: []\n  }),\n  created() {\n    this.get_choose_courses();\n  },\n  methods: {\n    delete_item(item) {\n      const index = this.courses.indexOf(item);\n      if (confirm(\"确定要退课?\")) {\n        var csplit=item.course.split(\"  \");\n        var tsplit=item.teacher.split(\"  \");\n        this.$http({\n          method: \"POST\",\n          url: \"/chooseCourse/cancelCourse\",\n          data:{\n              sid:this.sid,\n              cid:csplit[1],\n              tid:tsplit[1],\n              time:item.time\n          }\n        })\n          .then(res => {\n            if (res.status == 200) {\n              this.courses.splice(index, 1);\n            }\n          })\n          .catch(error => {\n            console.log(error.response);\n          });\n      }\n    },\n    get_choose_courses() {\n      if (this.sid == null) return;\n      this.$http({\n        method: \"get\",\n        url: \"/ocourse/getChooseCourse?sid=\" + this.sid\n      })\n        .then(res => {\n          for (var k = 0; k < res.data.length; k++) {\n            var pn = res.data[k].chosennum + \"/\" + res.data[k].maxnum;\n            var c = res.data[k].cname + \"  \" + res.data[k].cid;\n            var t = res.data[k].tname + \"  \" + res.data[k].tid;\n            this.$set(this.courses, k, {\n              pnum: pn,\n              course: c,\n              teacher: t,\n              time: res.data[k].time,\n              place: res.data[k].place,\n              score: res.data[k].score\n            });\n          }\n        })\n        .catch(error => {\n          console.log(error.response);\n        });\n    }\n  }\n};\n</script>\n\n<style scoped>\n</style>\n"],"sourceRoot":"src/components"}]}